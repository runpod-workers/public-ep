{
  "openapi": "3.1.0",
  "info": {
    "title": "FLUX-1-Dev API",
    "version": "1.0",
    "description": "High-quality AI image generation using Black Forest Labs FLUX-1-Dev model with advanced capabilities"
  },
  "servers": [
    {
      "url": "https://api.runpod.ai/v2",
      "description": "Production server"
    },
    {
      "url": "https://dev-api.runpod.ai/v2",
      "description": "Development server"
    }
  ],
  "paths": {
    "/black-forest-labs-flux-1-dev/{run_type}": {
      "post": {
        "summary": "FLUX-1-Dev Image Generation",
        "description": "Generate high-quality images using Black Forest Labs FLUX-1-Dev model - open-weights model with advanced capabilities and higher quality output",
        "operationId": "flux-1-dev",
        "parameters": [
          {
            "name": "run_type",
            "in": "path",
            "schema": {
              "type": "string",
              "enum": [
                "run",
                "runsync"
              ],
              "default": "runsync"
            },
            "required": true,
            "description": "Execution type: 'run' for async, 'runsync' for synchronous"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "input": {
                    "type": "object",
                    "properties": {
                      "prompt": {
                        "type": "string",
                        "description": "Text description of the image to generate.",
                        "minLength": 1,
                        "maxLength": 1000,
                        "example": "a beautiful landscape with mountains and a lake at sunset, photorealistic, highly detailed"
                      },
                      "negative_prompt": {
                        "type": "string",
                        "description": "Text description of what to avoid in the generated image.",
                        "default": "",
                        "maxLength": 500,
                        "example": "blurry, low quality, distorted, cartoon, anime"
                      },
                      "height": {
                        "type": "integer",
                        "description": "Height of the generated image in pixels. Must be multiple of 64. Range: 256-2048",
                        "default": 1024,
                        "minimum": 256,
                        "maximum": 2048,
                        "multipleOf": 64,
                        "example": 1024
                      },
                      "width": {
                        "type": "integer",
                        "description": "Width of the generated image in pixels. Must be multiple of 64. Range: 256-2048",
                        "default": 1024,
                        "minimum": 256,
                        "maximum": 2048,
                        "multipleOf": 64,
                        "example": 1024
                      },
                      "num_inference_steps": {
                        "type": "integer",
                        "description": "Number of denoising steps. More steps generally produce higher quality. Range: 1-100",
                        "default": 50,
                        "minimum": 1,
                        "maximum": 100,
                        "example": 50
                      },
                      "guidance_scale": {
                        "type": "number",
                        "description": "Guidance scale. Higher values follow the prompt more closely. Range: 1.0-30.0",
                        "default": 7.5,
                        "minimum": 1.0,
                        "maximum": 30.0,
                        "example": 7.5
                      },
                      "seed": {
                        "type": "integer",
                        "description": "Random seed for reproducible results. Use 0 for random seed.",
                        "default": 0,
                        "minimum": 0,
                        "maximum": 2147483647,
                        "example": 42
                      },
                      "max_sequence_length": {
                        "type": "integer",
                        "description": "Maximum sequence length for text encoding. Higher values allow for longer prompts.",
                        "default": 512,
                        "minimum": 128,
                        "maximum": 1024,
                        "example": 512
                      }
                    },
                    "required": [
                      "prompt"
                    ],
                    "additionalProperties": false
                  },
                  "webhook": {
                    "type": "string",
                    "format": "uri",
                    "description": "URL endpoint to receive a webhook call on job complete, fail, or timeout.",
                    "example": "https://your-domain.com/webhook"
                  }
                },
                "required": ["input"],
                "additionalProperties": false
              },
              "examples": {
                "basic_generation": {
                  "summary": "Basic high-quality generation",
                  "value": {
                    "input": {
                      "prompt": "a beautiful landscape with mountains and a lake at sunset, photorealistic, highly detailed",
                      "height": 1024,
                      "width": 1024,
                      "num_inference_steps": 50,
                      "guidance_scale": 7.5,
                      "seed": 0
                    }
                  }
                },
                "advanced_generation": {
                  "summary": "Advanced generation with all parameters",
                  "value": {
                    "input": {
                      "prompt": "a futuristic cityscape with flying cars and neon lights, cyberpunk style, highly detailed, 8k resolution",
                      "negative_prompt": "blurry, low quality, distorted, cartoon, anime, watermark",
                      "height": 1344,
                      "width": 768,
                      "num_inference_steps": 75,
                      "guidance_scale": 8.5,
                      "seed": 12345,
                      "max_sequence_length": 768
                    },
                    "webhook": "https://your-domain.com/webhook"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string",
                      "description": "Unique job identifier",
                      "example": "flux-dev-12345-abcde-67890"
                    },
                    "status": {
                      "type": "string",
                      "enum": ["IN_QUEUE", "IN_PROGRESS", "COMPLETED", "FAILED"],
                      "description": "Current job status",
                      "example": "COMPLETED"
                    },
                    "delayTime": {
                      "type": "integer",
                      "description": "Time in milliseconds before job processing started",
                      "example": 2500
                    },
                    "executionTime": {
                      "type": "integer",
                      "description": "Time in milliseconds taken to generate the image",
                      "example": 45000
                    },
                    "output": {
                      "type": "object",
                      "properties": {
                        "image_url": {
                          "type": "string",
                          "format": "uri",
                          "description": "URL of the generated high-quality image",
                          "example": "https://image.runpod.ai/gen-images/flux-dev/sample-image-hq.jpeg"
                        },
                        "message": {
                          "type": "string",
                          "description": "Human-readable message about the generation result",
                          "example": "High-quality image generated successfully with FLUX-1-Dev"
                        },
                        "status": {
                          "type": "string",
                          "enum": ["success", "failed", "error"],
                          "description": "Generation status",
                          "example": "success"
                        }
                      },
                      "required": ["image_url", "message", "status"]
                    },
                    "workerId": {
                      "type": "string",
                      "description": "Identifier of the worker that processed the job",
                      "example": "flux-dev-worker-gpu-01"
                    }
                  },
                  "required": ["id", "status"]
                },
                "examples": {
                  "async_result": {
                    "summary": "Async execution result",
                    "value": {
                      "id": "flux-dev-12345-abcde-67890",
                      "status": "IN_QUEUE"
                    }
                  },
                  "sync_result": {
                    "summary": "Synchronous execution result",
                    "value": {
                      "delayTime": 2500,
                      "executionTime": 45000,
                      "id": "flux-dev-12345-abcde-67890",
                      "output": {
                        "image_url": "https://image.runpod.ai/gen-images/flux-dev/sample-image-hq.jpeg",
                        "message": "High-quality image generated successfully with FLUX-1-Dev",
                        "status": "success"
                      },
                      "status": "COMPLETED",
                      "workerId": "flux-dev-worker-gpu-01"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request - Invalid input parameters",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "example": "INVALID_INPUT"
                    },
                    "message": {
                      "type": "string",
                      "example": "Invalid input parameters: prompt too long"
                    },
                    "details": {
                      "type": "object",
                      "properties": {
                        "field": {
                          "type": "string",
                          "example": "prompt"
                        },
                        "reason": {
                          "type": "string",
                          "example": "Exceeds maximum length of 1000 characters"
                        }
                      }
                    }
                  },
                  "required": ["error", "message"]
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized - Invalid or missing API key",
            "content": {
              "text/plain": {
                "example": "Unauthorized"
              }
            }
          },
          "429": {
            "description": "Too Many Requests - Rate limit exceeded",
            "content": {
              "text/plain": {
                "example": "Too Many Requests"
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string",
                      "example": "INTERNAL_SERVER_ERROR"
                    },
                    "message": {
                      "type": "string",
                      "example": "An unexpected error occurred during image generation"
                    }
                  },
                  "required": ["error", "message"]
                }
              }
            }
          }
        },
        "tags": ["Advanced Image Generation"],
        "security": [
          {
            "BearerAuth": []
          }
        ]
      }
    }
  },
  "components": {
    "securitySchemes": {
      "BearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "description": "RunPod API key as Bearer token"
      }
    }
  },
  "tags": [
    {
      "name": "Advanced Image Generation",
      "description": "High-quality AI-powered image generation with advanced parameters"
    }
  ]
}